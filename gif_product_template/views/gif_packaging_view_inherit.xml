<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="gif_packaging_view_inherit" model="ir.ui.view">
        <field name="name">gif.product.packaging.inherit.view</field>
        <field name="model">product.packaging</field>
        <field name="inherit_id" ref="product.product_packaging_form_view"/>
        <field name="arch" type="xml">
            
            <xpath expr="//field[@name='sales']" position="after">
                <field name="gif_packbed" />
                <field name="gif_numbt"/>
                <field name="gif_packagesxt"/>
            </xpath>
            
        </field>
    </record>
</odoo>