<?xml version='1.0' encoding='utf-8'?>
<odoo>
  <record id="vista_form" model="ir.ui.view">
    <field name="name">stock.picking.form</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_form"> </field>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='origin']" position="before">
        <field name="gif_prueba_boton" readonly='1' invisible='1'/>
      </xpath>
      
      <xpath expr="//field[@name='origin']" position="before">
        <field name="gif_select_all" />
      </xpath>
      <xpath expr="//notebook//page[@name='operations']//tree//field[@name='quantity_done']" position="before">
        
              <field name="gif_real_stockpicking"/>
                           
      </xpath>




    </field>



  </record>

</odoo>