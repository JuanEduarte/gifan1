<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="gif_acountMoveFieldIEPS" model="ir.ui.view">
        <field name="name">gif.accountMoveFieldIEPS</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='price_subtotal']" position = "before" > 
                <field name = "gif_AccountMoveIeps" force_save="1"/>
                <field name = "gif_IepsDisplay" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>