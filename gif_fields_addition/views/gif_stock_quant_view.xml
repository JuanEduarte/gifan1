<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- stock.quant inherit tree view -->
        <record id="stock_quants_view0_id" model="ir.ui.view">
            <field name="name">stock.quant.inherit.view.tree</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"/>
            <field name="arch" type="xml">
                
                <xpath expr="//tree/field[@name='lot_id']" position="before">
                    <field name="cct_expiration_date"/>
                </xpath>

            </field>
        </record>

        <!-- stock.quant inherit tree view -->
        <record id="stock_quants_view1_id" model="ir.ui.view">
            <field name="name">stock.quant.inherit.view.tree</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
            <field name="arch" type="xml">
                
                <xpath expr="//tree/field[@name='lot_id']" position="before">
                    <field name="cct_expiration_date"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>