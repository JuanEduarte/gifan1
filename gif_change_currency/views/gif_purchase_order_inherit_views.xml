<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="purchase_order_inherit_form_view_id_ve" model="ir.ui.view">
        <field name="name">purchase.order.inherit.view.form.ve</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='picking_type_id']" position="after">
                <field name="gif_own_currency_check_purchase"/>
                <field name="gif_own_inverse_currency_purchase"/>
            </xpath>
        </field>
    </record>
</odoo>